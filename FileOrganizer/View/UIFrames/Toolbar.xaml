<Page x:Class="FileOrganizer.View.UIFrames.Toolbar"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FileOrganizer.View.UIFrames"
      mc:Ignorable="d" 
      d:DesignHeight="20" d:DesignWidth="300"
      Title="Toolbar">

    <Grid>
        <ToolBarTray>
            <ToolBar DataContext="{StaticResource VModel}">
                <Button Content="Outer Folder" FontFamily="Arial" Command="{Binding ToParentDirectory}"/>
            </ToolBar>
        </ToolBarTray>
    </Grid>
</Page>
